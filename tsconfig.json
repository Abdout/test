{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/model/connect/db.ts", "src/app/api/report/route.js", "src/app/header/page.jsx", "src/component/report/header/form.tsx", "src/component/report/header/table.tsx", "src/app/report/page.jsx", "src/component/project/list.tsx", "src/component/modal/project/add.jsx", "src/component/modal/project/create.jsx", "src/component/project/delete.tsx", "src/app/api/report/footer/route.js", "src/provider/project.tsx"],
  "exclude": ["node_modules"]
}
